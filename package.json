{
  "name": "rpd7-task",
  "version": "1.0.0",
  "description": "YellowPages App",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "install:all": "concurrently \"npm install --prefix client\" \"npm install --prefix server\"",
